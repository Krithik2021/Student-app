pipeline {
    agent any

    stages {
        stage('Pull the Code') {
            steps {
                echo 'Pulling code from Repo'
            }
        }
        stage('Compile') {
            steps {
                echo 'Compiling Code'
            }
        }
        stage('Pre-Deployment') {
            steps {
                echo 'Pre Deployment'
            }
        }
    }
}
